@model DrugsSystem.WebUI.Models.OrderUnits.OrderResult
@{
    ViewBag.Title = "OrderResult";
}

<h2>OrderResult</h2>

<ul>
    @for (int i = 0; i < Model.Names.Count; i++)
    {
        <li>
            <div>@Model.Names[i]</div>
            <div>@Model.Values[i]</div>
        </li>
    }
</ul>